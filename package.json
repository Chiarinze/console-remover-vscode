{
  "name": "console-remover-vscode",
  "displayName": "Console Remover",
  "publisher": "chiarinze",
  "description": "Removes all console statements from current file or entire workspace.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "consoleRemover.removeCurrentFile",
        "title": "Console Remover: Remove consoles from current file"
      },
      {
        "command": "consoleRemover.removeWorkspaceConsoles",
        "title": "Console Remover: Remove consoles from entire workspace"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chiarinze/console-remover-vscode.git"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack --mode production",
    "watch": "webpack --watch",
    "package": "npx @vscode/vsce package",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.105.0",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.36.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.102.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "glob": "^11.0.3"
  }
}
